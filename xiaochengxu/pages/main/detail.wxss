/* pages/main/detail.wxss */

page {
  min-width: 320px;
  font: 14px/150% "微软雅黑", Verdana, Arial, Helvetica, sans-serif;
  color: #222;
  -webkit-text-size-adjust: none;
  padding: 0;
  margin: 0;
  background-color: #f8f8f8;
}
.container {
  padding: 0 20px;
}
.service-item {
  display: block;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #e8e8e8;
}

.title {
  line-height: 30px;
}

.cover {
  font-size: 18px;
  font-weight: 700;
}

.msection-title {
  font-size: 18px;
  font-weight: 700;
  margin: 0px 0 20px 0;
  padding-left: 15px;
  background-size: 5px 25px;
  position: relative;
}

.msection-title image {
  margin-left: -30rpx;
  width: 10rpx;
  height: 40rpx;
  position: absolute;
  text-align: left;
}

.table {
  border: 0px solid darkgray;
  padding-bottom: 20rpx;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: left;
  height: 3rem;
  align-items: left;
  
}

.td {
  width: 100%;
  justify-content: left;
  text-align: left;
  border: 1px solid darkgray;
}

.bg-w {
  background: snow;
}

.bg-g {
  background: #e6f3f9;
}

.th {
  width: 100%;
  justify-content: center;
  background: #36f;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  border: 1px solid darkgray;
}

.service-tags{
	margin: 6rpx 0;
}
.service-tags text{
	padding: 6rpx 16rpx;
	border: 1rpx solid #f7982a;
	line-height: 1;
	color: #f7982a;
	font-size: 28rpx;
	border-radius: 4rpx;
	margin-right: 10rpx;
	margin-bottom: 10rpx;
	white-space: nowrap;
}
